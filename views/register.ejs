<% include partials/header %>
<div class="container">
    <div class="jumbotron">
            <form action="/register" method="POST">
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label for="text">Type Username</label>
                            <input type="text" class="form-control" name="username" placeholder="JohnDoe20">
                        </div>
                        <div class="form-group col-md-4">
                            <label for="text">First Name</label>
                            <input type="text" class="form-control" name="firstname" placeholder="John">
                        </div>
                        <div class="form-group col-md-4">
                            <label for="text">Last Name</label>
                            <input type="text" class="form-control" name="lastname" placeholder="Doe">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                           <label for="email">Email Address</label>
                           <input type="email" class="form-control" name="emailAddress" placeholder="john@company.com">
                        </div>
                        <div class="form-group col-md-6">
                           <label for="email">Confirm Email Address</label>
                           <input type="email" class="form-control" name="confirmEmail">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                           <label for="password">Password</label>
                           <input type="password" class="form-control" name="password" placeholder="in lakhs">
                        </div>
                        <div class="form-group col-md-6">
                           <label for="password">Confirm Password</label>
                           <input type="password" class="form-control" name="confirmPassword">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="sel1">Gender</label>
                            <select class="form-control" id="sel1" name="gender">
                              <option>Male</option>
                              <option>Female</option>
                              <option>Others</option>
                            </select>
                          </div> 
                        <div class="form-group col-md-9">
                            <label for="inputAddress2">Address</label>
                            <input type="text" class="form-control" name="address">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                           <label for="netMoney">City</label>
                           <input type="text" class="form-control" name="city">
                        </div>
                        <div class="form-group col-md-4">
                           <label for="address">District</label>
                           <input type="text" class="form-control" name="district">
                        </div>
                        <div class="form-group col-md-4">
                            <label for="inputZip">Pin-Code</label>
                            <input type="number" class="form-control" name="pinCode">
                         </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                           <label for="netMoney">Phone Number</label>
                           <input type="number" class="form-control" name="mobileNo" placeholder="9413...">
                        </div>
                        <div class="form-group col-md-4">
                           <label for="Phone">Alternate Phone Number</label>
                           <input type="number" class="form-control" name="alternateMobileNo" placeholder="7647...">
                        </div>
                        <div class="form-group col-md-4">
                            <label for="adminCode">if Admin</label>
                            <input type="string" class="form-control" name="adminCode">
                        </div>
                    </div>
                    <button type="submit" class="btn btn-primary">Register</button>
            </form>
    </div>

</div>

<%include partials/footer %>